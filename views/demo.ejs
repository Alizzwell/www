<!DOCTYPE html>
<html lang="en">

<head>
	<meta http-equiv="X-UA-Compatible" content="IE=9">

	<link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
	<link rel="stylesheet" href="/stylesheets/style.css">
	<link rel="stylesheet" type="text/css" href="http://codemirror.net/lib/codemirror.css"/>
	
	<script src="/javascripts/jquery-2.2.4.min.js"></script>
	<script src="/javascripts/d3.min.js"></script>
	<script src="/javascripts/array.visualize.js"></script>
	<script src="/javascripts/this-play.js"></script>
	
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular.min.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular-animate.js"></script>
    <script src="http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-1.3.3.js"></script>
	<script src="http://codemirror.net/lib/codemirror.js"></script>
	<script src="http://codemirror.net/mode/scheme/scheme.js"></script>
      <script src="http://codemirror.net/mode/javascript/javascript.js"></script>
      <script src="http://codemirror.net/mode/xml/xml.js"></script>
	<script src="https://rawgithub.com/angular-ui/ui-codemirror/bower/ui-codemirror.min.js"></script>
	
	<script src="/javascripts/app.js"></script>
	<script src="/javascripts/clike.js"></script>
	<script src="/javascripts/active-line.js"></script>
	<script src="/javascripts/searchcursor.js"></script>
	<script src="/javascripts/mark-selection.js"></script>
	
</head>

<body ng-app="this-play.demo" >
	<%- include nav.ejs %>
	<div  id="body" class="container-fluid">
		<div class="wrapper" ng-controller="DemoCtrl" ng-cloak>
			<div  ng-hide="visualize">
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-primary">Menu</label>
							<label class="btn btn-primary">순차정렬</label>
							<label class="btn btn-primary">카테고리</label>
					</div>
				</div>
				
				<div class="row">
					<div class="col-md-12">
						<h4>카테고리</h4>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="arrayChk"> Array
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="sortChk"> Sort
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="searchChk"> Search
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="listChk"> Linked List
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="treeChk"> Tree
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="graphChk"> Graph
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" ng-model="dpChk"> Dynamic Programming
							</label>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'array'" ng-click=btnAlgorClick()>
								<img src="array.jpg" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>Array<h4>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'sort'" ng-click=btnAlgorClick()>
								<img src="sort.jpg" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>Sort<h4>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'search'" ng-click=btnAlgorClick()>
								<img src="search.png" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>Search<h4>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'list'" ng-click=btnAlgorClick()>
								<img src="list.jpg" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>List<h4>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'tree'" ng-click=btnAlgorClick()>
								<img src="tree.png" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>Tree<h4>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'graph'" ng-click=btnAlgorClick()>
								<img src="graph.png" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>Graph<h4>
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">
							<label class="btn btn-default" ng-model="selectAlgor" uib-btn-radio="'dp'" ng-click=btnAlgorClick()>
								<img src="dp.png" class="img-rounded" alt="Cinque Terre" width="240" height="160">
							</label>
							<h4>Dynamic Programming<h4>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="/javascripts/demo.js"></script>
</body>
</html>
